    <startEvent id="UserAccessHomescreen" name="User Access Homescreen"></startEvent>
    <serviceTask id="IsClientInRegistrationServiceId" name="Is Client In Registraion" activiti:expression="#{IsClientInRegistrationService.execute(execution)}"></serviceTask>
